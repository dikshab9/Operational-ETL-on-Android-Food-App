<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>FoodAppServlet</servlet-name>
        <servlet-class>edu.cmu.diksha.FoodAppServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FoodAppServlet</servlet-name>
        <url-pattern>/getFoodAppServlet1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MongoDBServlet</servlet-name>
        <servlet-class>edu.cmu.diksha.MongoDBServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MongoDBServlet</servlet-name>
        <url-pattern>/getDashboard</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>getFoodAppServlet1</welcome-file>
    </welcome-file-list>
</web-app>